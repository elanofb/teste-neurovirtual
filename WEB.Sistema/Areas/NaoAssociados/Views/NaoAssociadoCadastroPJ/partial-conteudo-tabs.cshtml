@model WEB.Areas.NaoAssociados.ViewModels.NaoAssociadoCadastroPJForm

@{ Html.RenderPartial("aba-dados-cadastrais");}

@if (Model.Associado.id > 0) {

    if (Model.ConfiguracaoAssociadoPJ.flagAbaContato == true) {
        { Html.RenderPartial("abas/aba-contatos", Model.Associado.idPessoa); }
    }

    { Html.RenderPartial("~/Areas/NaoAssociados/Views/NaoAssociadoCadastroPF/abas/aba-documentos.cshtml", Model.Associado); }

    { Html.RenderPartial("~/Areas/NaoAssociados/Views/NaoAssociadoCadastroPF/abas/aba-historico.cshtml", Model.Associado); }
    
    { Html.RenderPartial("~/Areas/Associados/Views/associado/aba-movimentacao.cshtml", Model.Associado.id); }

    if (Model.ConfiguracaoAssociadoPJ.flagAbaPedidos == true) {
        { Html.RenderPartial("~/Areas/Associados/Views/AssociadoCadastroPF/abas/aba-pedidos.cshtml", Model.Associado); }
    }

    if (Model.ConfiguracaoAssociadoPJ.flagAbaVeiculos == true) {
        { Html.RenderPartial("~/Areas/Associados/Views/AssociadoCadastroPF/abas/aba-veiculos.cshtml", Model.Associado); }
    }
    
    if (Model.ConfiguracaoAssociadoPJ.flagDadosOutrasOrganizacoes == true) {
        { Html.RenderPartial("~/Areas/Associados/Views/AssociadoCadastroPF/abas/aba-outras-organizacoes.cshtml", Model.Associado); }
    }
    
    { Html.RenderPartial("abas/aba-conta-bancaria", Model.Associado); }

}
