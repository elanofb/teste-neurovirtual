@model WEB.Areas.Permissao.ViewModels.PerfilAcessoForm

@{
    ViewBag.titlePage = String.Concat("Grupo de usuários ", Model.PerfilAcesso.descricao);
}

<div class="row">
    <div class="col-sm-12">
        <div class="nav-tabs-custom">
            <ul class="nav nav-tabs">
                <li class="active"><a href="#tab-1" data-toggle="tab">Principal</a></li>
            </ul>
            
            <div class="tab-content">
                <div id="tab-1" class="tab-pane active">

                    @Html.infoCadastro(Model.PerfilAcesso.id, Model.PerfilAcesso.dtCadastro, Model.PerfilAcesso.dtAlteracao)

                    @{Html.RenderPartial("partial-dados-principais", Model);}
                    
                </div>
            </div>
            <div class="clearfix"></div>
        </div>
    </div>
</div>


