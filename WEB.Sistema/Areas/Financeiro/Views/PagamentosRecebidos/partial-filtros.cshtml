    <div class="col-md-4 col-sm-6">
        <label>Data Vencimento</label><br/>
        <div class="input-group input-group-sm">
            @Html.TextBox("dtVencimentoInicio", UtilRequest.getString("dtVencimentoInicio"), new {@class = "form-control input-sm input-datepicker", alt = "date"})
            <span class="input-group-addon">-</span>
            @Html.TextBox("dtVencimentoFim", UtilRequest.getString("dtVencimentoFim"), new {@class = "form-control input-sm input-datepicker", alt = "date"})
        </div>
    </div>
    <div class="clearfix visible-sm"></div>

    <div class="col-md-4 col-sm-6">
        <label>Data Pagamento</label><br />
        <div class="input-group input-group-sm">
            @Html.TextBox("dtPagamentoInicio", UtilRequest.getString("dtPagamentoInicio"), new { @class = "form-control input-sm input-datepicker", alt = "date" })
            <span class="input-group-addon">-</span>
            @Html.TextBox("dtPagamentoFim", UtilRequest.getString("dtPagamentoFim"), new { @class = "form-control input-sm input-datepicker", alt = "date" })
        </div>
    </div>
    <div class="clearfix hidden-sm"></div>

    <div class="col-md-4 col-sm-6">
        <label>Tipo de Receita</label>
        @Html.DropDownList("idTipoReceita", TipoReceitaHelper.getInstance.selectList(UtilRequest.getInt32("idTipoReceita")), "Todos", new {@class = "form-control input-sm"})
    </div>
    <div class="clearfix visible-sm"></div>

    <div class="col-md-4 col-sm-6">
        <label>Meio de Pagamento</label>
        @Html.DropDownList("idMeioPagamento", MeioPagamentoHelper.getInstance.selectList(UtilRequest.getInt32("idMeioPagamento")), "Todos", new { @class = "form-control input-sm" })
    </div>

    <div class="col-md-2 col-sm-6">
        <label>Exibição</label>
        @Html.DropDownList("idTipoSaida", TipoSaidaHelper.selectListTipoSaida(UtilRequest.getString("idTipoSaida")), new {@class = "form-control input-sm"})
    </div>
    <div class="clearfix visible-sm"></div>

    <div class="form-group col-md-2 col-sm-12">
        <label>Buscar</label>
        @Html.buttonSearch(UtilRequest.getString("valorBusca"))
    </div>
    <div class="clearfix"></div>
}
	    