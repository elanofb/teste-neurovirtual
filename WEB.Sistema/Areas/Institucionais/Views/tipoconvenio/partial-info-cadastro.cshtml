@model WEB.Areas.Institucionais.ViewModels.TipoConvenioForm

@if (Model.TipoConvenio.id > 0) {
	<div class="form-horizontal">
		<div class="col-lg-1 col-xs-2">
			<label class="no-margin">ID</label>
			<input type="text" class="form-control input-sm" readonly="readonly" value="@Model.TipoConvenio.id" />
		</div>
		<div class="col-lg-2 col-xs-3">
			<label class="no-margin">Cadastrado em:</label>
			<input type="text" class="form-control input-sm" readonly="readonly" value="@Model.TipoConvenio.dtCadastro" />
		</div>
		<div class="col-lg-2 col-xs-3">
			<label class="no-margin">Alterado em:</label>
			<input type="text" class="form-control input-sm" readonly="readonly" value="@Model.TipoConvenio.dtAlteracao" />
		</div>

	</div>
	<div class="clearfix"></div>
}
