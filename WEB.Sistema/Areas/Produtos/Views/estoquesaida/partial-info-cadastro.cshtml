@model WEB.Areas.Produtos.ViewModels.EstoqueSaidaForm

@if (Model.EstoqueSaida.id > 0)
{ 
<div class="form-horizontal">
	<div class="col-md-6">
		<a class="btn btn-default bg-gray"><strong>ID:</strong> @Model.EstoqueSaida.id</a> &nbsp;
		<a class="btn btn-default bg-gray"><strong>Cadastro:</strong> @Model.EstoqueSaida.dtCadastro</a> &nbsp;
		<a class="btn btn-default bg-gray"><strong>Alterado em:</strong> @Model.EstoqueSaida.dtAlteracao</a>
	</div>
</div>
<div class="clearfix"></div><br />
}