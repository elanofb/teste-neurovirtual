@model IList<DAL.Organizacoes.UsuarioOrganizacao>


@{
    Layout = null;
}

<div class="col-xs-12 box box-solid internal-default">
	
	<div class="table-responsive">
		<div class="dataTables_wrapper form-inline" role="grid">
			<table class="table table-bordered table-striped dataTable">
				<thead>
					<tr role="row">
						<th>Associação</th>
						<th class="width-130">Cadastro</th>
						<th width="80">&nbsp;</th>
					</tr>
				</thead>
				<tbody>
					@if (Model.Count > 0){
						foreach (var Item in Model){
							<tr>
								<td>@(Item.Organizacao?.Pessoa?.nome)</td>
								<td class="text-center">@Item.dtCadastro.exibirData(true)</td>
                                <td class="text-left box-acoes">
                                   <a href="javascript:;" onclick="UsuarioOrganizacao.excluirOrganizacaoVinculada(this);" data-id="@Item.id" data-idusuario="@Item.idUsuario" data-url="@Url.Action("excluir-organizacao-vinculada", "UsuarioOrganizacao", new { area = "UsuariosOrganizacoes" })" title="Excluir Registro" data-toggle="tooltip"><i class="fa fa-trash"></i></a>
                                </td> 
							</tr>
						}
					}else{
						<tr>
							<td class="text-center" colspan="3">Nenhum registro cadastrado</td>
						</tr>
					}
				</tbody>
			</table>
		</div>
	</div>

</div>
<div class="clearfix"></div>
