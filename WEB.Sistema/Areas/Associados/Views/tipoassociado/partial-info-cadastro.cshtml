@model WEB.Areas.Associados.ViewModels.TipoAssociadoForm
@if (Model.TipoAssociado.id > 0 && Model.TipoAssociado.dtAlteracao != DateTime.MinValue) {
    <div class="form-horizontal">
        <div class="col-xs-2 col-lg-1">
            <label class="no-margin">ID</label>
            <input type="text" class="form-control input-sm" readonly="readonly" value="@Model.TipoAssociado.id" />
        </div>
        <div class="col-xs-3 col-lg-2">
            <label class="no-margin">Cadastrado em:</label>
            <input type="text" class="form-control input-sm" readonly="readonly" value="@Model.TipoAssociado.dtCadastro" />
        </div>
        <div class="col-xs-3 col-lg-2">
            <label class="no-margin">Alterado em:</label>
            <input type="text" class="form-control input-sm" readonly="readonly" value="@Model.TipoAssociado.dtAlteracao" />
        </div>

    </div>
    <div class="clearfix"></div>
}