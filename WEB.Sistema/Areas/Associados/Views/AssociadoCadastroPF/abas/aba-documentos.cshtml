@model DAL.Associados.Associado
@using DAL.Entities

<div class="tab-pane" id="tab-documentos">
    <div class="col-md-5">
        <h4 class="separator-form-icon">
            <i class="fa-edit far"></i> Cadastrar Documentos/Arquivos
        </h4>
        <div id="boxLoadFormDocumentos" class="carregando content-load" data-url="@Url.Action("editar-documento", "arquivo", new {area = "arquivos", idReferencia = Model.id, entidade = EntityTypes.ASSOCIADO})"></div>
    </div>

    <div class="col-md-6 col-md-offset-1">
        <h4 class="separator-form-icon">
            <i class="fa-edit far"></i> Documentos e Arquivos Cadastrados
        </h4>
        <div id="boxLoadListaDocumentos" class="" data-url="@Url.Action("listar-galeria-documentos", "arquivo", new {area = "arquivos", idReferencia = Model.id, entidade = EntityTypes.ASSOCIADO})"></div>
    </div>
    <div class="clearfix"></div>
    <br />

    <div class="col-xs-12">
        <h4 class="separator-form-icon">
            <i class="fa-edit far"></i> Documentos e Arquivos do histórico de Relacionamento
        </h4>
        <div id="boxLoadListaDocumentosRelacionamento" class="carregando content-load" data-url="@Url.Action("listar-galeria-documentos", "PessoaRelacionamentoArquivo", new {area = "Pessoas", Model.idPessoa})"></div>
    </div>

    <div class="clearfix"></div>
    <br />

</div>