@model WEB.Areas.ConfiguracoesCarteirinha.ViewModels.ConfiguracaoCarteirinhaForm

@if(Model.ConfiguracaoCarteirinha.id > 0) {
    
    <div class="col-sm-4 col-md-2">
        <label>Cadastro</label>
        @Html.boxInfo(Model.ConfiguracaoCarteirinha.dtCadastro.exibirData(true), "text-center")
    </div>
    <div class="col-sm-6 col-md-4">
        <label>Usuário</label>
        @Html.boxInfo(Model.ConfiguracaoCarteirinha.UsuarioCadastro?.nome, "text-left")
    </div>
    <div class="clearfix"></div><br/>

}