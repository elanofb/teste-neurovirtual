@model WEB.Areas.Relacionamentos.ViewModels.OcorrenciaRelacionamentoForm

@if (Model.OcorrenciaRelacionamento.id > 0) {
    <div class="form-horizontal">
        <div class="col-xs-2">
            <label class="no-margin">ID</label>
            <input type="text" class="form-control input-sm" readonly="readonly" value="@Model.OcorrenciaRelacionamento.id" />
        </div>
        <div class="col-xs-3">
            <label class="no-margin">Cadastrado em:</label>
            <input type="text" class="form-control input-sm" readonly="readonly" value="@Model.OcorrenciaRelacionamento.dtCadastro" />
        </div>
        <div class="col-xs-3">
            <label class="no-margin">Alterado em:</label>
            <input type="text" class="form-control input-sm" readonly="readonly" value="@Model.OcorrenciaRelacionamento.dtAlteracao" />
        </div>

    </div>
    <div class="clearfix"></div>
}