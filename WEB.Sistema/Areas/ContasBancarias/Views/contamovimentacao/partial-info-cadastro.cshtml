@model WEB.Areas.ContasBancarias.ViewModels.ContaMovimentacaoForm

@if (Model.ContaMovimentacao.id > 0)
{ 
    <div class="form-horizontal">
	    <div class="col-md-1">
		    <label>ID</label>
		    <input type="text" class="form-control input-sm" readonly="readonly" value="@Model.ContaMovimentacao.id" />
	    </div>
	    <div class="col-md-2">
		    <label>Cadastrado em:</label>
		    <input type="text" class="form-control input-sm" readonly="readonly" value="@Model.ContaMovimentacao.dtCadastro" />
	    </div>
	    <div class="col-md-2">
		    <label>Alterado em:</label>
		    <input type="text" class="form-control input-sm" readonly="readonly" value="@Model.ContaMovimentacao.dtAlteracao" />
	    </div>

    </div>
    <div class="clearfix"></div><br />
}